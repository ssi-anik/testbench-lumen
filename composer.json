{
    "name": "anik/testbench-lumen",
    "description": "Testing Helper for Lumen package development",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Syed Sirajul Islam Anik",
            "email": "sirajul.islam.anik@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2|^8.0",
        "laravel/lumen": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^8.0|^9.3",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Anik\\Testbench\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anik\\Testbench\\Tests\\": "tests/"
        }
    }
}
